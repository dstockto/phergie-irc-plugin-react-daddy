{
    "minimum-stability": "dev",
    "require": {
        "phergie/phergie-irc-bot-react": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "4.0.*",
        "phake/phake": "1.0.*",
        "codeclimate/php-test-reporter": "0.1.*@dev",
        "satooshi/php-coveralls": "v0.6.1"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "sebastianbergmann/phpcov",
                "version": "1.1.0",
                "dist": {
                    "url": "https://github.com/sebastianbergmann/phpcov/archive/1.1.0.zip",
                    "type": "zip"
                },
                "source": {
                    "url": "https://github.com/sebastianbergmann/phpcov.git",
                    "type": "git",
                    "reference": "1.1.0"
                },
                "bin": [
                    "phpcov.php"
                ]
            }
        }
    ],
    "autoload": {
        "psr-4": {
            "Phergie\\Irc\\Plugin\\React\\Daddy\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phergie\\Irc\\Tests\\Plugin\\React\\Daddy\\": "tests"
        }
    }
}
