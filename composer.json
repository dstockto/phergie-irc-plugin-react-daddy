{
    "name": "phergie/phergie-irc-plugin-react-daddy",
    "type": "library",
    "description": "Phergie IRC bot plugin responding to Who's your *? phrases",
    "keywords": ["irc","phergie"],
    "homepage": "https://github.com/dstockto/phergie-irc-plugin-react-daddy",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "David Stockton",
            "email": "phergie-daddy@davidstockton.com",
            "homepage": "http://davidstockton.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.5",
        "phergie/phergie-irc-bot-react": "~2"
    },
    "require-dev": {
        "phpunit/phpunit": "4.0.*",
        "phake/phake": "1.0.*",
        "codeclimate/php-test-reporter": "0.1.*@dev",
        "satooshi/php-coveralls": "v0.6.1"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "sebastianbergmann/phpcov",
                "version": "1.1.0",
                "dist": {
                    "url": "https://github.com/sebastianbergmann/phpcov/archive/1.1.0.zip",
                    "type": "zip"
                },
                "source": {
                    "url": "https://github.com/sebastianbergmann/phpcov.git",
                    "type": "git",
                    "reference": "1.1.0"
                },
                "bin": [
                    "phpcov.php"
                ]
            }
        }
    ],
    "autoload": {
        "psr-4": {
            "Phergie\\Irc\\Plugin\\React\\Daddy\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phergie\\Irc\\Tests\\Plugin\\React\\Daddy\\": "tests"
        }
    }
}
